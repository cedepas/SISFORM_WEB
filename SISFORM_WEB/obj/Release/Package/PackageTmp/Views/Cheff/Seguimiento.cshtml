
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script>
        var siteActive = document.getElementById("docs-section-id");
    siteActive.value = "Seguimiento";
</script>


@{
    ViewBag.Title = "Evaluacion";
}
@Styles.Render("~/plugins/Handsontable/handsontable.full.css")
@Styles.Render("~/Styles/apexcharts.css")
@section styles{
    <style>
        .fondo_texto {
            color: #020202;
        }

        .dark-mode, .dark-mode h1, .dark-mode h2, .dark-mode h3, .dark-mode h4, .dark-mode h5, .dark-mode h6 {
            color: #ff4d4f;
        }
    </style>
}
<div class="content">
    <div class="title"><h5>Evaluación Sensorial</h5></div>
    <br />
    <button id="btnGuardar" class="btn btn-primary bg-green shadow hover-moveup">Guardar</button>
    <div class="row">
        <div>
            <div class="fondo_texto" id="ResultadoTabla">
            </div>
            <br />

        </div>
    </div>

</div>
<div class="content">
    <hr class="mb-20" />
</div>
<div class="mw-full">
    <div class="card p-0 bg-very-dark-dm">
        <div id="divTabla" class="table-responsive">
            <table id="tablaDatos" class="table table-inner-bordered">
            </table>
        </div>
    </div>
</div>
@section scripts{
    @Scripts.Render("~/plugins/Handsontable/handsontable.full.js")
    @Scripts.Render("~/plugins/Handsontable/numbro/numbro.js")
    @Scripts.Render("~/plugins/Handsontable/numbro.js")
    @Scripts.Render("~/plugins/Handsontable/languages/es-MX.js")
    @Scripts.Render("~/Scripts/ConsultaEvalCheff.js")

}
