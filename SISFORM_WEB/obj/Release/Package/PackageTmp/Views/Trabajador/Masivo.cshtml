@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!-- Styles start -->
@section styles{
    
}
<!-- Styles end -->
<!-- Modals start -->
@section modals{
    <script>
        var siteActive = document.getElementById("docs-section-id");
        siteActive.value = "masivo";
    </script>
}
<!-- Modals end -->

<div class="content">
    <div class="title"><h5>@ViewBag.Title</h5></div>
    <div class="row">
        <div class="col-sm-12">
            <div class="form-row">
                <div class="col-sm-3">
                    <label for="dtFecAsi" class="required">Fecha de asignacion</label>
                    <input type="date" class="form-control form-control-sm T H F" id="dtFecAsi" required="required" />
                </div>
                <div class="col-sm-6" id="fechaPrueba" style="display:inline">
                    <label for="txtFile">Cargar trabajadores(Excel)</label>
                    <input type="file" class="form-control form-control-sm F" id="txtFile" required="required" />
                </div>
            </div>
        </div>        
    </div>
    <div class="text-right">
        <input class="btn btn-success" id="btnCargar" type="button" value="Cargar">
    </div>
</div>
<div class="content">
    <hr class="mb-20" />
</div>
<div class="mw-full">
    <div class="card p-0 bg-very-dark-dm">
        <div id="divTabla" class="table-responsive">
            <table id="tablaDatos" class="table table-inner-bordered">
            </table>
        </div>
    </div>
</div>
<iframe name="print_frame" width="0" height="0" frameborder="0" src="about:blank"></iframe>
@section scripts{
    @Scripts.Render("~/Scripts/ConsultaTrabajadorMasivo.js")
}
